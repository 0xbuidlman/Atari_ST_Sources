#include "pgem.h"
#include "pmisc.h"

#include "machine.h"


EXTERN THEGLO		D;


	VOID
sc_read(pscrap)
	LONG		pscrap;
{
	LSTCPY( ( BYTE *)pscrap, ( BYTE *)&D.g_scrap[0]);
}


	VOID
sc_write(pscrap)
	LONG		pscrap;
{
	LSTCPY( ( BYTE *)&D.g_scrap[0], ( BYTE *)pscrap);
}

@


