#define	LOC_MULTIWAIT		115
#define	I_TEXT_EXTENT		116
#define	I_TEXT_WIDTH		117
#define	TIM_VECX		118

#define	LOAD_ALPHA_TEXT		119
#define	UNLOAD_ALPHA_TEXT	120
