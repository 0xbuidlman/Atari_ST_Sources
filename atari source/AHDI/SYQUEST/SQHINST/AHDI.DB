b #2 ._sasi_me ; b #3 .decided
b #4 ._sasi_rw ; b #5 .ahrw7
b #6 ._sasi_bp ; b #7 .retbpb

alias br2 "print -n mediach (wpeek (+ `sp 4));defer g"
alias br3 "print mc returns `d0;defer g"
alias br4 "print -n rwabs (wpeek (+ `sp 4)) (lpeek (+ `sp 6)) \
	(wpeek (+ `sp a)) (wpeek (+ `sp c)) (wpeek (+ `sp e));defer g"
alias br5 "print rw returns `d0;defer g"
alias br6 "print -n getbpb (wpeek (+ `sp 4));defer g"
alias br7 "print bpb returns `d0;defer g"

alias unahdi "nb #2;nb #3;nb #4;nb #5;nb #6;unalias br2 br3 br4 br5 br6 br7"
