T%=0
Fileselect "\*.*","cbhd.sys",Fi$
If Fi$<>""
  For I=0 To 7
    For J=0 To 1
      Print "Target ";I;" Ger„t ";J;": J/N ";
      Repeat
        A$=Upper$(Input$(1))
      Until A$="J" Or A$="N"
      Print A$
      If A$="J"
        T%=T% Or 2^(I*2+J)
      Endif
    Next J
  Next I
  Input "OK (J/N)";A$
  A$=Upper$(A$)
  If A$="J"
    '
    Open "U",#1,Fi$
    Seek #1,70
    Out #1,T%/256
    Out #1,T% And 255
    Close #1
  Endif
Endif
