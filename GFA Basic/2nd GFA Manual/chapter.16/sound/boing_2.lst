' *** BOING_2.LST ***                                2nd GFA-Manual, page 16-13
> PROCEDURE sound_boing_2(pause)
  '
  ' *** Boing-sound; echo hangt af van pause
  '
  LOCAL i
  FOR i=15 DOWNTO 0
    SOUND 1,i,1,3
    SOUND 2,i,5,3
    SOUND 3,i,1,4
    WAVE 7
    PAUSE pause
    SOUND 3,i,1,5
    PAUSE pause
  NEXT i
  WAVE 0,0
RETURN
' *********
'
