' 'JOUE' un fichier HTA
FILESELECT "","*.HTA",s$
INPUT #99,nbp&,net&,mode
cpp=0
DIM xt&(nbp&*net&),yt&(nbp&*net&)
FOR nbj=0 TO nbp&-2
  FOR cp=0 TO net&-1
    INPUT #99,xt&(cpp),yt&(cpp)
    INC cpp
  NEXT cp
NEXT nbj
FOR nbj=0 TO nbp&-2
  FOR cp=0 TO net&-1
    PLOT xt&(cpp),yt&(cpp)
    INC cpp
  NEXT cp
  PAUSE 15
NEXT nbj
~INP(2)
