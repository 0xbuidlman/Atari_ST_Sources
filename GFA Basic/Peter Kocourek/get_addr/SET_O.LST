OPEN "r",#1,"c_addr.o"
INPUT "Name of the GFA function you want the address of:",a$
SEEK #1,50
do_out("_"+UPPER$(LEFT$(a$,7)))
INPUT "Name of the C function that returns the address:",a$
SEEK #1,36
do_out(LEFT$(a$,8))
CLOSE
'
PROCEDURE do_out(q$)
  LOCAL i&
  FOR i&=1 TO 8
    '    PRINT MID$(q$,i&,1);
    PRINT #1,MID$(q$,i&,1);
  NEXT i&
RETURN
