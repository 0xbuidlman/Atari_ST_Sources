> PROCEDURE plusslatt
  FOR i%=1 TO 15
    tal1%=INT(RND*10)+1
    tal2%=INT(RND*10)+1
    PRINT tal1%;"+";tal2%
    INPUT "vad blir ovanst†ende tal";ssvar%
    IF ssvar%=tal1%+tal2%
      PRINT "bravo"
    ELSE
      PRINT tal1%+tal2%;"ska de vara "
    ENDIF
  NEXT i%
RETURN
> PROCEDURE minuslatt
  FOR i%=1 TO 15
    tal1%=INT(RND*10)+1
    tal2%=INT(RND*10)+1
    PRINT tal1%;"-";tal2%
    INPUT "vad blir ovanst†ende tal";ssvar%
    IF ssvar%=tal1%-tal2%
      PRINT "bravo"
    ELSE
      PRINT tal1%-tal2%;"ska de vara "
    ENDIF
  NEXT i%
RETURN
