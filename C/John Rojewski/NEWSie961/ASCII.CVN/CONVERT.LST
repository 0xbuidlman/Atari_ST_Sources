INLINE convert%,3354
RESERVE
RESERVE 100000
FILESELECT "*.*","",input$
FILESELECT "*.*","",output$
INPUT "Entr‚e ",input&
INPUT "Sortie ",output&
IF EXIST(input$)
  OPEN "i",#1,input$
  l%=LOF(#1)
  CLOSE #1
  buffer%=MALLOC(l%)
  BLOAD input$,buffer%
  dummy&=C:convert%(L:buffer%,L:l%,W:input&,W:output&)
  IF dummy&=0
    BSAVE output$,buffer%,l%
  ELSE
    PRINT "Erreur"
  ENDIF
  ~MFREE(buffer%)
ENDIF
RESERVE
