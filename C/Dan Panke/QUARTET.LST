PROCEDURE quartet(song%,vset%)
  '
  ' NOTE: This routine does NOT work uncompiled!
  '
  IF xquartet%=0
    RESERVE 20000
    xquartet%=EXEC(3,"singsong.dat","","")
    ~MFREE(HIMEM)
    ~MFREE(xquartet%)
    RESERVE
    ~MSHRINK(xquartet%,16384)
    ADD xquartet%,256
  ENDIF
  {xquartet%+12}=song%
  {xquartet%+16}=vset%
  @jsr(xquartet%)
  SOUND 1,0,0,0,0
  SOUND 2,0,0,0,0
  SOUND 3,0,0,0,0
  ~MFREE(xquartet%-256)
RETURN
