> PROCEDURE sin1
  .
  .71*62=points(4402)
  .
  v=5
  a=0
  aa=0
  FOR t=0 TO 70
    a=aa
    FOR y=0 TO 61
      x=16+15*COSQ(a)
      PSET x,y,1
      ADD a,v
    NEXT y
    ADD aa,v
    VSYNC
    CLS
  NEXT t
  .
RETURN
> PROCEDURE sin2
  .
  .16*62=points(992)
  .
  FOR x=0 TO 15
    FOR y=0 TO 61 STEP 2
      PSET 15-x,y,1
    NEXT y
    FOR y=1 TO 61 STEP 2
      PSET 16+x,y,1
    NEXT y
    VSYNC
  NEXT x
  .
RETURN
