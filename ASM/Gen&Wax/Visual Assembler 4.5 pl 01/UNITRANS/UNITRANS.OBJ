		; FIRST_EXT_DX,FIRST_EXT_DY;FIRST_INT_VISIBLE_W,FIRST_INT_VISIBLE_H;FIRST_CONTENTS_W,FIRST_CONTENTS_H;FIRST_CONTENTS_DX,FIRST_CONTENTS_DY;FIRST_INT_BORDERS_W,FIRST_INT_BORDERS_H
