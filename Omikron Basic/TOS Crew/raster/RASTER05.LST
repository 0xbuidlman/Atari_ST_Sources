 Mchar%L= MEMORY(2048)
 BLOAD "c:\omikron.bas\demo\raster\6x8ni___.fnt",Mchar%L
 'BLOAD "6x8ni___.fnt",Mchar
 FOR I%=0 TO 127
   Len%=0
   FOR J%=0 TO 5
     Kar%= PEEK(Mchar%L+I%*8+J%)
     FOR K%=0 TO 7
       IF (Kar% AND (1 SHL (7-K%))) AND (K%>Len%) THEN
         Len%=K%
       ENDIF
     NEXT K%
   NEXT J%
   POKE Mchar%L+I%*8+6,Len%+1
 NEXT I%
 POKE Mchar%L+32*8+6,5
 Mtxt%L= MEMORY(10240)
 BLOAD "c:\omikron.bas\demo\raster\raster.txt",Mtxt%L
